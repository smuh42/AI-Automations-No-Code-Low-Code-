    {
      "parameters": {
        "jsCode": "const item = $input.first().json;\n\n// Gmail uses base64url sometimes (- and _). Normalize it.\nfunction decodeBase64Url(data) {\n  if (!data) return \"\";\n  const normalized = data.replace(/-/g, \"+\").replace(/_/g, \"/\");\n  return Buffer.from(normalized, \"base64\").toString(\"utf8\");\n}\n\n// Recursively walk parts and collect text bodies\nfunction collectTextFromPayload(payload) {\n  let out = \"\";\n\n  if (!payload) return out;\n\n  // Some emails have body directly\n  if (payload.body && payload.body.data) {\n    out += \"\\n\" + decodeBase64Url(payload.body.data);\n  }\n\n  // Most emails have nested parts\n  if (payload.parts && Array.isArray(payload.parts)) {\n    for (const p of payload.parts) {\n      out += collectTextFromPayload(p);\n    }\n  }\n\n  return out;\n}\n\nconst subject = item.Subject || item.subject || \"\";\nconst payload = item.payload || item.Payload;\n\nconst bodyText = collectTextFromPayload(payload);\n\n// Combine subject + body\nconst text = `${subject}\\n${bodyText}`;\n\n// Flexible number extraction:\n// Handles \"199 Impressions\", \"Impressions 199\", line breaks, etc.\nfunction extractMetric(name) {\n  // 1) \"199 impressions\"\n  let m = text.match(new RegExp(String.raw`(\\d+)\\s*${name}s?`, \"i\"));\n  if (m) return parseInt(m[1], 10);\n\n  // 2) \"impressions 199\"\n  m = text.match(new RegExp(String.raw`${name}s?\\s*(\\d+)`, \"i\"));\n  if (m) return parseInt(m[1], 10);\n\n  return 0;\n}\n\nlet impressions = extractMetric(\"impression\");\nlet reactions = extractMetric(\"reaction\");\nlet comments = extractMetric(\"comment\");\n\n// Fallback for impressions from subject like \"199 views\"\nif (!impressions) {\n  const mv = subject.match(/(\\d+)\\s+views?/i);\n  if (mv) impressions = parseInt(mv[1], 10);\n}\n\nreturn [\n  {\n    impressions,\n    reactions,\n    comments,\n    subject,\n    timestamp: new Date().toISOString(),\n    // optional debug: keep short excerpt so you can confirm parsing sees content\n    debug_sample: text.slice(0, 300)\n  }\n];\n"
      },
